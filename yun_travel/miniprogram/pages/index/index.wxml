<import src="../../components/travelCircel/travelCircel.wxml"/>
<view class="container">
  <view class="adInfos">
    <swiper wx:if="{{ads.length}}" autoplay="true" indicator-dots="true" circular="true">
      <block wx:for="{{ads}}" wx:for-item="ad" wx:key="ad._id">
        <swiper-item>
          <image src="{{ad.pic}}" data-id="{{ad._id}}" mode="aspectFill"/>
        </swiper-item>
      </block>
    </swiper>
  </view>
  <view class="hotTravelCircel">
    <view class="title">
      <view class="txt f32 fw fc3">最新</view>
      <navigator class="more f32 fc9" open-type="switchTab" url="../travel/travel">More</navigator>
    </view>
    <block wx:for="{{travels}}" wx:for-item="item" wx:key="item.id">
      <template is="travelCircel" data="{{item:item}}"/>
    </block>
  </view>
</view>